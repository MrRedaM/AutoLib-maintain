<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.clovertech.autolib">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logojaune"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logojaune"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.Agent_APP">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name=".ui.SampleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.SplashScreen">
        </activity>
        <activity android:name=".ui.OnBoardingScreens" />
        <activity android:name=".ui.MainActivity"/>
        <activity android:name=".ui.login.LoginActivity">

        </activity>


    </application>

</manifest>