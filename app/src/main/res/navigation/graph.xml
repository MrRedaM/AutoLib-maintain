<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.autolibdz_ui.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_detailTache"
            app:destination="@id/detailTache" />
    </fragment>
    <fragment
        android:id="@+id/navigation_userProfil"
        android:name="com.example.autolibdz_ui.ui.userProfil.UserProfilFragment"
        android:label="fragment_profil"
        tools:layout="@layout/fragment_profil" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.autolibdz_ui.ui.notifications.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.autolibdz_ui.ui.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/detailTache"
        android:name="com.example.autolibdz_ui.DetailTache"
        android:label="fragment_detail_tache"
        tools:layout="@layout/fragment_detail_tache" >
        <action
            android:id="@+id/action_detailTache_to_ajouterMateriel"
            app:destination="@id/ajouterMateriel" />
    </fragment>
    <fragment
        android:id="@+id/ajouterMateriel"
        android:name="com.example.autolibdz_ui.AjouterMateriel"
        android:label="fragment_ajouter_materiel"
        tools:layout="@layout/fragment_ajouter_materiel" />
</navigation>